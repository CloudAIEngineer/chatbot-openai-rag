# Pinecone API key (replace with your actual key in .env)
PINECONE_API_KEY=pcsk_sample_key

# OpenAI API key (replace with your actual key in .env)
OPENAI_API_KEY=sk-proj-sample_key

# Original OpenAI model identifier (used for evaluation)
ORIGINAL_MODEL=ft:gpt-4o-mini-YYYY-MM-DD

# Fine-tuned OpenAI model identifier (update accordingly)
FINETUNED_MODEL=ft:gpt-4o-mini-YYYY-MM-DD:sample-project::sample-id

# Pinecone index name
PINECONE_DATABASE=sample-index-name

# DynamoDB table name for session storage (if applicable)
SESSION_TABLE_NAME=SampleChatbotSessions

# Embedding model for vector database
EMBEDDING_MODEL=text-embedding-ada-002

# AWS ECR Docker image URI for serverless deployment
DOCKER_IMAGE_URI=sample-account-id.dkr.ecr.eu-central-1.amazonaws.com/sample-repo@sha256:samplehash